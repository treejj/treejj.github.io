using System;

namespace HelloWorld
{
  class Program
  {
    static void Main(string[] args)
    {
      Hinhhoc h1 = new Hinhhoc();
      HinhCN h2 = new HinhCN();
      h1.CANH = 3;
      h1.Ve();
      h2.Ve();
      h2.canh = 3;
    }
  }
  class Hinhhoc{
  //1.truong
 	int canh;
  //2.thuoc tinh
  public int CANH{
	get{ return canh;}
    set{ canh = value;}
   }
  //3.phuong thuc
  	public void Ve(){
    	Console.WriteLine("Toi la hinh hoc.");
  
}
}
	class HinhCN : Hinhhoc {
    	public void Ve(){
        	Console.WriteLine("Toi la hinh chu nhat");
}
}
}
